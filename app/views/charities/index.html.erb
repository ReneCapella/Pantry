
<div class="container">
  <h2>Food Donation Centers in my area</h2>
</div>
<div class="container-fluid shadow">
  <iframe
    width="100%"
    height="300px"
    frameborder="0" style="border:0"
    src="https://maps.google.com/maps?q=food+pantry,near+me&output=svembed" allowfullscreen>
  </iframe>
</div>
<% if @donated_items%>
  <div class="container">
    <h4>Items Marked for Donation</h4>
    <%@donated_items.each do |item|%>
      <p><%=item.name%></p>
    <%end%>
  </div>
<% end %>
